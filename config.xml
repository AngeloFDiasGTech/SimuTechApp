<?xml version='1.0' encoding='utf-8'?>
<widget id="ailton.simutech" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SimuTechApp</name>
    <description>Aplicativo Simutech!</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Ailton
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="electron">
        <preference name="ElectronSettingsFilePath" value="res/electron/settings.json" />
    </platform>

    <icon src="res/electron/simuIcon.png" platform="electron" density="mdpi" />
</widget>

